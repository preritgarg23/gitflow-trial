{
    "plugins": [],
    "changelog-sections": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "revert",
        "section": "Reverts"
      },
      {
        "type": "docs",
        "section": "Documentation",
        "hidden": true
      },
      {
        "type": "style",
        "section": "Styles",
        "hidden": true
      },
      {
        "type": "chore",
        "section": "Miscellaneous Chores",
        "hidden": true
      },
      {
        "type": "refactor",
        "section": "Code Refactoring",
        "hidden": true
      },
      {
        "type": "test",
        "section": "Tests",
        "hidden": true
      },
      {
        "type": "build",
        "section": "Build System",
        "hidden": false
      },
      {
        "type": "ci",
        "section": "Continuous Integration",
        "hidden": false
      }
    ],
    "draft": false,
    "prerelease": false,
    "skip-github-release": false,
    "separate-pull-requests": true,
    "group-pull-request-title-pattern": "chore: release ${version}",
    "release-search-depth": 400,
    "commit-search-depth": 500,
    "sequential-calls": false,
    "packages": {
      ".": {
        "release-type": "python",
        "pull-request-title-pattern": "chore: release ${version}",
        "pull-request-header": ":robot: A new release is ready for review :rocket:",
        "include-v-in-tag": false,
        "extra-files": [
        ]
      }
    }
  }